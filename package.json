{
  "name": "cheffist",
  "version": "1.0.0",
  "private": true,
  "description": "Eleventy-first food data site scaffold",
  "engines": {
    "node": ">=20 <23"
  },
  "scripts": {
    "dev": "eleventy --serve",
    "subscribe:server": "node scripts/subscribe-server.js",
    "data:build": "node scripts/build-data.js",
    "clean:output": "node scripts/clean-output.js",
    "report:pseo": "node scripts/build-pseo-report.js",
    "build": "npm run data:build && npm run clean:output && eleventy && npm run report:pseo",
    "test": "node --test"
  },
  "devDependencies": {
    "@11ty/eleventy": "^3.1.2"
  },
  "dependencies": {
    "csv-parse": "^6.1.0"
  }
}
